{
  "name": "webpack-boilerplate",
  "version": "5.0.0",
  "description": "Plain webpack 5 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "dev": "webpack serve --progress --env dev --config webpack.dev.js",
    "build": "webpack --progress --env prod --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.1",
    "babel-loader": "8.2.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.3.1",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "node-sass": "5.0.0",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "webpack": "5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0"
  }
